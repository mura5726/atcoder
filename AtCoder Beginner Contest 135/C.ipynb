   "source": [
    "N=int(input())\n",
    "A=list(map(int,input().split()))\n",
    "B=list(map(int,input().split()))\n",
    "\n",
    "ans=0\n",
    "for i in range(N):\n",
    "    p=min(A[i]+A[i+1],B[i])\n",
    "    ans=ans+p\n",
    "    if A[i]<=B[i]:\n",
    "        A[i+1]=max(0,A[i+1]-B[i]+A[i])\n",
    "print(ans)"
