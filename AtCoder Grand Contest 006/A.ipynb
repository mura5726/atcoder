   "source": [
    "N=int(input())\n",
    "s=input()\n",
    "t=input()\n",
    "ans=N*2\n",
    "for i in range(0,N):\n",
    "    for j in range(0,N):\n",
    "        if N-i+N-j >= N:\n",
    "            ST=s[0:N-i]+t[j:N]\n",
    "            if ST[0:N]==s and ST[-N:]==t:\n",
    "                ans=min(len(ST),ans)\n",
    "print(ans)"
   ]
