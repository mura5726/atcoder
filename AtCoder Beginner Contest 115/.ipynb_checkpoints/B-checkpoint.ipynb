    "import numpy as np\n",
    "\n",
    "N = int(input())\n",
    "plist = []\n",
    "for i in range(N):\n",
    "    plist.append(int(input()))    \n",
    "\n",
    "plist2 = np.array(plist)\n",
    "\n",
    "tf = np.ones(N,dtype='bool')   \n",
    "\n",
    "tf[plist.index(max(plist))] = False\n",
    "\n",
    "print(int(sum(plist2[tf]) + max(plist)/2))"
   ]
