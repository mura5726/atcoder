   "source": [
    "N, P = map(int, input().split())\n",
    "cnt = int(P**(1/(N)))\n",
    "\n",
    "if P == 1:\n",
    "    print(1)\n",
    "elif N == 1:\n",
    "    print(P)\n",
    "\n",
    "else:\n",
    "    for i in range(1, cnt):\n",
    "        a = P/(i**N)\n",
    "        if a.is_integer():\n",
    "            ans = i\n",
    "        else:\n",
    "            continue\n",
    "    print(ans)"
   ]
