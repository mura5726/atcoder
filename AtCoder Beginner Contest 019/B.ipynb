    "s=input()\n",
    "c=0\n",
    "t=0\n",
    "ans=[]\n",
    "for n in range(len(s)-1):\n",
    "    if s[n+1]==s[n]:\n",
    "        c+=1\n",
    "    else:\n",
    "        ans.append(s[n]+str(c+1))\n",
    "        t+=c+1\n",
    "        c=0\n",
    "ans.append(s[n+1]+str(len(s)-t))\n",
    "print(''.join(ans))"
   ]
