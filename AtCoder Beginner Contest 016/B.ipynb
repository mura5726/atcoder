   "source": [
    "a,b,c=map(int,input().split())\n",
    "f=[]\n",
    "\n",
    "if a+b == c:\n",
    "    f.append(1)\n",
    "else:\n",
    "    f.append(0)\n",
    "\n",
    "if a-b == c:\n",
    "    f.append(1)\n",
    "else:\n",
    "    f.append(0)\n",
    "\n",
    "if all(f):\n",
    "    print('?')\n",
    "elif not any(f):\n",
    "    print('!')\n",
    "elif f[0]:\n",
    "    print('+')\n",
    "else:\n",
    "    print('-')"
   ]
